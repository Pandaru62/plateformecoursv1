{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}
    <h1>Ajouter un document</h1>

    {{ form_errors(form) }}

    {{ form_start(form) }}

        {{ form_row(form.seance) }}
        {{ form_row(form.type) }}
        {{ form_row(form.titre) }}
        {{ form_row(form.description) }}
        {{ form_row(form.path) }}

        <button type="submit" class="btn btn-primary mt-3">Ajouter</button>
    {{ form_end(form) }}
{% endblock %}
